public class circledes
{
	public static void main (String args[])
	{
		for(int i=-10;i<=10;i++)
		{
			for(int j=-10;j<=10;j++)
			{
				if(((i*i)+(j*j))==25)
				{
					System.out.print("*");
					for(i=0;i<5;i++)
					{
						for(j=0;j<5;j++)
						{
							System.out.print("*");
						}
					}
				}
				else
					System.out.print(" ");
			}
			System.out.println();
		}
	}
}
